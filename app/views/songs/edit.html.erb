<h1>Edit Song</h1>
<%= form_for @song do |f| %>

<%= label_tag :name%><br>
<%= text_field_tag :'song[name]'%><br>

<%= label_tag :artist_id%><br>
<%= text_field_tag :'song[artist_id]'%><br>

<%= label_tag :genre_id%><br>
<%= text_field_tag :'song[genre_id]'%><br>

<%= f.submit "Update Song"%>

<% end %>
